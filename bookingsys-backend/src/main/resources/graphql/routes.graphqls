type Route{
    id : ID,
    fromLocation : Location,
    toLocation : Location
    fromLocationName : String,
    toLocationName : String,
    vehicles : [Vehicle],
    subCountyName : String,
    countyName : String
}


input RouteDto{
    scheduleId : String!,
    fromLocation : LocationDto,
    toLocation : LocationDto,
    fromLocationName : String,
    toLocationName : String
    subCountyName : String!,
    countyName : String!
}